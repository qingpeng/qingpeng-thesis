	Journal = {Appl Environ Microbiol},
	Journal-Full = {Applied and environmental microbiology},
	Mesh = {Algorithms; Bacteria; Bayes Theorem; Classification; Databases, Nucleic Acid; Phylogeny; RNA, Ribosomal; RNA, Ribosomal, 16S},
	Month = {Aug},
	Number = {16},
