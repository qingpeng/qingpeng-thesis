	Month = {Jul},
	Number = {7},
	Pages = {689-95},
	Pmid = {18463690},
	Pst = {ppublish},
	Title = {The tragedy of the uncommon: understanding limitations in the analysis of microbial diversity},
	Volume = {2},
	Year = {2008},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/ismej.2008.44}}

